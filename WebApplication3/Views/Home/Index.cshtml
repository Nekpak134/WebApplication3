@model List<WebApplication3.Models.empmodel>
@{
    ViewBag.Title = "Home Page";
}
<h2>********table data********</h2>
<table class="table table-bordered table-hover table-responsive">
    <tr>
        <th class="bg-success">id</th>
        <th class="bg-success">fname</th>
        <th class="bg-success">lname</th>
        <th class="bg-success">dept</th>
        <th class="bg-success"></th>
        <th class="bg-success"></th>
        <th class="bg-success"></th>


    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.id</td>
    <td>@item.fname</td>
    <td>@item.lname</td>
    <td>@item.dept</td>
    <td><a class="btn btn-danger" href="@Url.Action("delete","home",new {id=item.id })">DELETE</a></td>
    <td><a class="btn btn-warning" href="@Url.Action("details","home",new {id=item.id })">DETAILS</a></td>
    <td><a class="btn btn-success" href="@Url.Action("edit","home",new {id=item.id })">EDIT</a></td>


</tr>
    }
</table>
